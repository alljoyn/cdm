<node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.allseenalliance.org/schemas/introspect.xsd">
    <interface name="org.alljoyn.SmartSpaces.Environment.WindStrength">
        <description language="en">This interface is for controlling wind strength of a device.</description>
        <annotation name="org.alljoyn.Bus.Secure" value="true"/>
        <property name="Version" type="q" access="read">
            <description language="en">Interface version</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="WindStrength" type="y" access="readwrite">
            <description language="en">Wind strength of a device</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="MaxStrength" type="y" access="read">
            <description language="en">Maximum strength allowed for target wind strenghth</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
        </property>
        <property name="AutoMode" type="y" access="readwrite">
            <description language="en">Represent enabled/disabled state of the auto mode.</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
    </interface>
</node>
