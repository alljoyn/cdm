<node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.allseenalliance.org/schemas/introspect.xsd">
    <interface name="org.alljoyn.SmartSpaces.Devices.Freezer">
        <description language="en">This interface provides capabilities to interact with specific functions and resources of Freezers</description>
        <annotation name="org.alljoyn.Bus.Secure" value="true"/>
        <property name="Version" type="q" access="read"/>
        <property name="RapidFreezing" type="y" access="read">
            <description language="en">Rapid Freezing status</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <method name="EnableRapidFreezingMode">
            <description language="en">Enable rapid freezing mode</description>
            <arg name="mode" type="b" direction="in">
                <description language="en">Rapid Freezing status to be set</description>
            </arg>
        </method>
    </interface>
</node>