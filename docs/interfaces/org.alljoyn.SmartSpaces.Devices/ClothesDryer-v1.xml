<node xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.allseenalliance.org/schemas/introspect.xsd">
    <interface name="org.alljoyn.SmartSpaces.Devices.ClothesDryer">
        <description language="en">This interface provides capabilities to interact with specific functions and resources of Clothes Dryers</description>
        <annotation name="org.alljoyn.Bus.Secure" value="true"/>
        <struct name="CyclePhaseInfo">
            <field name="cyclePhase" type="y"/>
            <field name="name" type="s"/>
        </struct>
        <struct name="OperationalCycleDescriptor">
            <field name="cycleId" type="q"/>
            <field name="name" type="s"/>
            <field name="description" type="s"/>
        </struct>
        <property name="Version" type="q" access="read"/>
        <property name="CyclePhaseId" type="y" access="read">
            <description language="en">Currently selected cycle phase identifier</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="SupportedCyclePhasesIds" type="ay" access="read">
            <description language="en">Supported cycle phase identifiers</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
        </property>
        <property name="OperationalCycleId" type="q" access="read">
            <description language="en">Currently selected operational cycle identifier</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="true"/>
        </property>
        <property name="SupportedOperationalCycleIds" type="aq" access="read">
            <description language="en">List of supported operational cycle identifiers</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
        </property>
        <property name="SelectableOperationalCycleIds" type="aq" access="read">
            <description language="en">List of selectable operational cycle identifiers</description>
            <annotation name="org.freedesktop.DBus.Property.EmitsChangedSignal" value="false"/>
        </property>
        <method name="GetCyclePhaseIdsInfo">
            <description language="en">Get cycle phases identifier info</description>
            <arg name="languageTag" type="s" direction="in">
                <description language="en">Language to be used in the output strings</description>
            </arg>
            <arg name="description" type="[CyclePhaseInfo]" direction="out">
                <description language="en">Cycle phases info list</description>
            </arg>
        </method>
        <method name="SetOperationalCycleId">
            <description language="en">Set operational cycle identifier</description>
            <arg name="cycleId" type="q" direction="in">
                <description language="en">Language to be used in the output strings</description>
            </arg>    
        </method>
        <method name="GetOperationalCyclesDescription">
            <description language="en">Get operational cycles description</description>
            <arg name="languageTag" type="s" direction="in">
                <description language="en">Language to be used in the output strings</description>
            </arg>
            <arg name="description" type="[OperationalCycleDescriptor]" direction="out">
                <description language="en">Operational cycles description</description>
            </arg>
        </method>
    </interface>
</node>